{
  "name": "react-fabric",
  "version": "0.0.1",
  "description": "Office UI Fabric components built with React",
  "main": "index.js",
  "scripts": {
    "build:src": "babel src --out-dir dist/",
    "build:example": "webpack --config ./example/webpack.config.js",
    "build": "parallelshell 'npm run build:src' 'npm run build:example'",
    "watch:src": "babel src -w --out-dir dist/",
    "watch:example": "webpack -w --config ./example/webpack.config.js",
    "watch": "parallelshell 'npm run watch:src' 'npm run watch:example'",
    "lint:src": "eslint -c .eslintrc --ext .js --ext .jsx src/",
    "lint:example": "eslint -c .eslintrc --ext .js --ext .jsx example/",
    "lint": "parallelshell 'npm run lint:src' 'npm run lint:example'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.1.3",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
    "eslint": "^1.3.1",
    "eslint-config-airbnb": "0.0.8",
    "eslint-plugin-react": "^3.3.1",
    "parallelshell": "^2.0.0",
    "webpack": "^1.12.0"
  }
}
